@model User


    <h1>Welcome to the Dojo Activity Center</h1>
    <div style="width:48%; display: inline-block; border: 2px solid black; padding:20px;">
    <form action="/register" method="post" class="col-6 mx-auto">
        <div class="form-group">
            <label asp-for="FName"></label>
            <input type="text" asp-for="FName" id="FName" class="form-control">
            <span class="text-danger" asp-validation-for="FName"></span>
        </div>
        <div class="form-group">
            <label asp-for="LName"></label>
            <input type="text" asp-for="LName" id="LName" class="form-control">
            <span class="text-danger" asp-validation-for="LName"></span>
        </div>
        <div class="form-group">
            <label asp-for="Email"></label>
            <input type="email" asp-for="Email" id="Email" class="form-control">
            <span class="text-danger" asp-validation-for="Email"></span>
        </div>
        <div class="form-group">
            <label asp-for="Password"></label>
            <input type="password" asp-for="Password" id="Password" class="form-control">
            <span class="text-danger" asp-validation-for="Password"></span>
        </div>
        <div class="form-group">
            <label asp-for="Confirm"></label>
            <input type="password" asp-for="Confirm" id="Confirm" class="form-control">
            <span class="text-danger" asp-validation-for="Confirm"></span>
        </div>
            <input type="submit" value="Register" class="btn btn-primary" style="float:right">
        
    </form>
    </div>